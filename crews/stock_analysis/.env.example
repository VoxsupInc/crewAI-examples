SERPER_API_KEY=KEY # https://serper.dev/ (free tier)
BROWSERLESS_API_KEY=KEY # https://www.browserless.io/ (free tier)
SEC_API_API_KEY=KEY # https://sec-api.io/ (free tier)

# CrewAI Settings
CREWAI_DISABLE_TELEMETRY=true

# OpenTelemetry / Jaeger Tracing Configuration
# Set to true to enable tracing to Jaeger
CREWAI_TRACING_ENABLED=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_EXPORTER_OTLP_PROTOCOL=grpc
OTEL_SERVICE_NAME=stock-analysis-crew

# AWS Bedrock Configuration
AWS_ACCESS_KEY_ID=your-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-secret-access-key-here
AWS_REGION=us-east-1
AWS_BEDROCK_MODEL=us.anthropic.claude-sonnet-4-5-20250929-v1:0

## Some other Anthropic models to consider
# AWS_BEDROCK_MODEL=us.anthropic.claude-haiku-4-5-20251001-v1:0
# AWS_BEDROCK_MODEL=anthropic.claude-3-5-sonnet-20240620-v1:0
# AWS_BEDROCK_MODEL=us.anthropic.claude-sonnet-4-20250514-v1:0

## Check https://docs.aws.amazon.com/bedrock/latest/userguide/models-supported.html for model names
